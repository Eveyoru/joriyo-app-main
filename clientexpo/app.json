{
  "expo": {
    "name": "Joriyo",
    "slug": "joriyo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/joiryologoicon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/joiryologoicon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#3498db"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sergio101.clientexpo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/joiryologoicon.jpg",
        "backgroundColor": "#3498db"
      },
      "package": "com.sergio101.clientexpo"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/joiryologoicon.jpg"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "joriyo",
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f41315ae-90bd-429a-9de3-8eb4cff37d80"
      }
    }
  }
}
